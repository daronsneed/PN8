{
  "buildCommand": "cd webapp && bun install && bun run build",
  "outputDirectory": "webapp/dist",
  "installCommand": "bun install",
  "framework": null,
  "functions": {
    "backend/api/index.ts": {
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/backend/api" },
    { "source": "/health", "destination": "/backend/api" }
  ]
}
