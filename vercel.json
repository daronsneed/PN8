{
  "buildCommand": "cd webapp && bun install && bun run build",
  "outputDirectory": "webapp/dist",
  "installCommand": "bun install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api" },
    { "source": "/health", "destination": "/api" }
  ]
}
